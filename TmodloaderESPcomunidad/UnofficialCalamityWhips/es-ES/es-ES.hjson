		Configs: {
			UnofficialCalamityWhipsConfig: {
				DisplayName: Configuración de Unofficial Calamity Whips
				Headers.Config: Opciones de Configuración

				WhipDamageModifier: {
					Label: Modificador de daño de látigo
					Tooltip:
						'''
						Modifica el daño de todos los látigos en este mod con el valor seleccionado.
						Úsalo para potenciar o debilitar el mod según tu preferencia.
						'''
				}

				AddArmorStats: {
					Label: Añadir Estadísticas de látigo a armaduras de Calamity
					Tooltip:
						'''
						Agrega estadísticas adicionales de látigo a algunas armaduras de Calamity mientras esté activado,
						para mejorar la viabilidad de los látigos.
						'''
				}

				AllowTagStacking: {
					Label: Habilitar Acumulación de marcas
					Tooltip:
						'''
						Permite que los látigos en este mod acumulen sus efectos de marca.
						Si está desactivado, cada nueva marca reemplazará a la anterior
						'''
				}

