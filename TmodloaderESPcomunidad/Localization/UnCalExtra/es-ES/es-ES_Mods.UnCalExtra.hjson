Configs: {
	UnCalConfig: {
		DisplayName: Unofficial Calamity Extra Config

		Headers: {
			Graphics: Graphics
			DeveloperContent: Developer Content
			Content: Content
		}

		UseBloom: {
			Label: Use Bloom
			Tooltip:
				'''
				Whether or not to apply a glowing, blurry effect to certain visuals.
				This can be configured whenever, however it is best to do so from within the menu.
				Suddenly disabling this while something is using it can cause the game to crash.
				THIS SHOULD NOT BE USED IF RETRO LIGHTING IS ON!
				'''
		}

		AllowScreenshake: {
			Label: Allow Screenshake
			Tooltip:
				'''
				Whether or not to allow certain visuals to have more of an impact by vibrating the screen.
				Turn this off, or the strength of it down if you have motion sickness.
				Can be configured whenever.
				'''
		}

		DebugMode: {
			Label: Debug Mode
			Tooltip: WARNING-YOU MAY BE FLOODED WITH MESSAGES RANDOMLY!
		}

		ScreenShakePower: {
			Label: Screen Shake Power
			Tooltip:
				'''
				How strong the use of screenshake should be.
				Can be configured whenever.
				'''
		}

		BloomPower: {
			Label: Bloom Power
			Tooltip: ""
		}

		BloomResolution: {
			Label: Bloom Resolution
			Tooltip: ""
		}

		UseWarp: {
			Label: Use Warp
			Tooltip:
				'''
				Whether or not to enhance some visuals with a screen warping effect.
				This can be configured whenever, however it is best to do so from within the menu.
				THIS SHOULD NOT BE USED IF RETRO LIGHTING IS ON!
				'''
		}

		UseHealingEffects: {
			Label: Use Healing Effects
			Tooltip:
				'''
				Whether or not to allow visuals to spawn when the player is healed.
				Can be configured whenever.
				'''
		}
	}
}

